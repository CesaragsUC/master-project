{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenTelemetryOptions": {
    "Environment": "development",
    "AppName": "Auth.Api",
    "Jaeger": {
      "AgentHost": "localhost",
      "AgentPort": 6831
    },
    "OtlExporter": {
      "Endpoint": "http://localhost:4317"
    },
    "GrafanaLoki": {
      "Endpoint": "http://localhost:3100"
    }
  },
  "keycloak": {
    "realm": "casoft",
    "auth-server-url": "https://keycloak-casoft.azurewebsites.net/realms/casoft/protocol/openid-connect",
    "ssl-required": "none",
    "resource": "casoftsystem",
    "verify-token-audience": true,
    "credentials": {
      "secret": ""
    },
    "use-resource-role-mappings": true,
    "confidential-port": 0,
    "policy-enforcer": {
      "credentials": {}
    }
  }
}