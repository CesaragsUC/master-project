{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenTelemetryOptions": {
    "Environment": "production",
    "AppName": "Auth.Api",
    "OtlExporter": {},
    "GrafanaLoki": {}
  },
  "Keycloak": {
    "realm": "casoft",
